if [ "--usage" == "$1" ] ; then
    echo "Fake Release Tool"
    exit 2
fi
if [ "--help" == "$1" ] ; then
    echo "--lots of complicated"
    echo "--help for the release tool"
    echo "--goes here"
    exit 2
fi

for flavor in DO_THIS NEXT_COMMAND ADJUST_FROBULTARS CALIBRATE_ECMA6 PARALLEL_WARP INPSECT_ALPHA_CHANNELS REMOVE_WHITESPACE; do
	echo $flavor...
	T=` expr $RANDOM % 2 `
	sleep $T
done
